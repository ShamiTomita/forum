<h1>Edit Your Stats!</h1>
<%= render partial: "layouts/errors", locals: { object: @user } %>
<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br>

  <%= f.label :age; %>
  <%= f.number_field :age %><br>
  <%= f.label :fav_exercise; %>
  <%= f.text_field :fav_exercise %><br>
  <%= f.label :gym; %>
  <%= f.text_field :gym %><br>

  <%= f.label "Profile Photo" %>
  <%= f.file_field :avatar%><br>
  <%=f.submit "Update"%>
<%end%>
