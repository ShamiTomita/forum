<h1>Please Signup here!</h1>
<%= render partial: "layouts/errors", locals: { object: @user } %>
<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br>
  <%= f.label :email; %>
  <%= f.password_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.label "Profile Photo" %>
  <%= f.file_field :avatar%><br>
  <%=f.submit "Signup"%>
<%end%>
